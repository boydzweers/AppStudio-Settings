{
    "sqltools.useNodeRuntime":true,
    "sqltools.languageServerEnv":{
       "PATH":"/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/home/user/.node_modules_global/bin"
    },
    "cds.checkDevKitInstalled":false,
    "cds.releaseNotes.showAutomatically":false,
    "java.jdt.ls.vmargs":"-DwatchParentProcess=false -noverify -XX:MaxDirectMemorySize=10M -XX:MaxMetaspaceSize=98632K -Xss1M -Xmx437943K -XX:+UseG1GC -XX:+UseStringDeduplication",
    "java.configuration.checkProjectSettingsExclusions":false,
    "java.configuration.updateBuildConfiguration":"automatic",
    "java.autobuild.enabled":false,
    "java.debug.settings.console":"internalConsole",
    "java.home":"/home/user/java11",
    "java.configuration.runtimes":[
       {
          "name":"JavaSE-11",
          "path":"/usr/lib/sapmachine-jdk/sapmachine-jdk-11.0.6.0.1",
          "default":true
       }
    ],
    "java.server.launchMode":"Standard",
    "java.recommendations.dependency.analytics.show":false,
    "java.semanticHighlighting.enabled":false,
    "files.exclude":{
       "**/.classpath":true,
       "**/.project":true,
       "**/.settings":true,
       "**/.factorypath":true
    },
    "ports.excludeExposeNotification":{
       "ports":[
          8000,
          8005,
          8009
       ],
       "ranges":[
          {
             "from":0,
             "to":0
          }
       ]
    },
    "spring-boot.ls.checkJVM":false,
    "spring-boot.ls.java.vmargs":[
       "-XX:MaxDirectMemorySize=10M",
       "-XX:MaxMetaspaceSize=98632K",
       "-Xss1M",
       "-Xmx437943K"
    ],
    "editor.autoSaveDelay":30000,
    "workbench.iconTheme":"vs-seti",
    "workbench.colorTheme":"Default Dark+",
    "editor.renderWhitespace":"all",
    "editor.insertSpaces":false,
    "welcome.showOnStartup":true,
    "editor.formatOnSave":true,
    "prettier.tabWidth":4,
    "prettier.useTabs":true,
    "xml.format.preserveAttributeLineBreaks":true,
    "prettier.printWidth":120,
    "java.import.exclusions":[
       "**/node_modules/**",
       "**/.metadata/**",
       "**/archetype-resources/**",
       "**/META-INF/maven/**"
    ],
    "sqltools.dependencyManager":{
       "packageManager":"npm",
       "installArgs":[
          "install"
       ],
       "runScriptArgs":[
          "run"
       ],
       "autoAccept":false
    },
    "json.schemas":[
       
    ],
    "actions":[
       
    ],
    "emmet.excludeLanguages":[
       "markdown"
    ],
    "emmet.preferences":{
       
    },
    "eslint.codeAction.showDocumentation":{
       "enable":true
    },
    "boot-java.remote-apps":[
       
    ]
 }